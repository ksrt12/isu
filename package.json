{
  "name": "isu",
  "version": "2.0",
  "private": true,
  "homepage": "https://isu.ksrt12.ru/",
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m \"$(git show -s --format='Update from %h: %s')\"",
    "start": "react-scripts start",
    "build": "react-scripts build && yarn userscripts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "userscripts": "tsc -p ./src/scripts/tsconfig.json && sed '2d' -i ./build/scripts/*.user.js",
    "server": "serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "sass": "^1.49.7"
  }
}